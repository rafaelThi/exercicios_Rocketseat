        <script> 
        function rtnsx(sexo) {


            if (sexo==="M"){
                return 'Homem';}
            else if(sexo==="F") {
                return 'Mulher';
            }else{
                return 'indefinido';                
            }
        }
        var x = rtnsx('M');
        console.log(x);
    </script>
    </body>

</html>

<html> 
    <head>
    <title> Curso 1 </title>

    </head>
    <body>
        <script> 
        function rtnsx(sexo) {
            switch (sexo){
                case 'M': 
                return 'Masculino';
                case 'm': 
                return 'Masculino';
                case 'F': 
                return 'Feminino';
                case 'f': 
                return 'Feminino';
                default: 
                return 'Indefinido';

            }
        }
        var x = rtnsx('g');
        console.log(x);
    </script>
    </body>

</html>

