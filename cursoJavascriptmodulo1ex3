    <script>
    var skills = ["Javascript", "ReactJS", "React Native"];
function habilidade(Skills){
        if (skills[0]=== 'Javascript') {
            console.log(true);
        } else if (skills[1]!== 'Javascript') {
            console.log(false);
        }else if (skills[2]!== 'Javascript') {
            console.log(false);
        }}

    habilidade()
</script>

